{
  "default_locale": "en",
  "description": "An Opera extension to use web Telegram React version",
  "background": {
    "scripts": []
  },
  "developer": {
    "name": "LeonardsonCC"
  },
  "icons": {
    "128": "icons/128.png",
    "19": "icons/19.png",
    "38": "icons/38.png",
    "48": "icons/48.png"
  },
  "manifest_version": 2,
  "name": "Telegram React Sidebar",
  "permissions": ["tabs", "activeTabs", "<all_urls>"],
  "sidebar_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_panel": "sidebar.html",
    "default_title": "Telegram React"
  },
  "version": "1.0.1",
  "update_url": "https://github.com/LeonardsonCC/opera-sidebar-extension-telegram-react"
}
